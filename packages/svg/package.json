  "description": "SVG for storytelling",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsc --watch",
    "build": "tsc",
    "test:unit": "NODE_ENV=test jest \"tests/unit/.*\\.test\\.ts\"",
    "test:e2e": "NODE_ENV=test playwright test \"tests/e2e/.*\\.spec\\.ts\"",
    "storybook": "NODE_ENV=test storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
